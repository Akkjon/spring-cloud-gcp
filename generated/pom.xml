<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
  <parent>
    <artifactId>spring-cloud-gcp</artifactId>
    <groupId>com.google.cloud</groupId>
    <version>3.5.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>

  <artifactId>generated-parent</artifactId>
  <packaging>pom</packaging>

  <dependencyManagement>
    <dependencies>
      <dependency>
        <groupId>com.google.cloud</groupId>
        <artifactId>global-spring-autoconfig-properties</artifactId>
        <version>3.5.0-SNAPSHOT</version>
      </dependency>
    </dependencies>
  </dependencyManagement>

  <modules>
    <module>workflows</module>
    <module>websecurityscanner</module>
    <module>webrisk</module>
    <module>vpcaccess</module>
    <module>vmmigration</module>
    <module>vision</module>
    <module>transcoder</module>
    <module>videointelligence</module>
    <module>cloudtrace</module>
    <module>texttospeech</module>
    <module>storagetransfer</module>
    <module>serviceusage</module>
    <module>servicemanagement</module>
    <module>servicedirectory</module>
    <module>servicecontrol</module>
    <module>privateca</module>
    <module>securitycenter</module>
    <module>secretmanager</module>
    <module>retail</module>
    <module>resourcesettings</module>
    <module>redis</module>
    <module>recommender</module>
    <module>recaptchaenterprise</module>
    <module>cloudprofiler</module>
    <module>policytroubleshooter</module>
    <module>osconfig</module>
    <module>orgpolicy</module>
    <module>notebooks</module>
    <module>networkmanagement</module>
    <module>networkconnectivity</module>
    <module>monitoring</module>
    <module>memcache</module>
    <module>managedidentities</module>
    <module>language</module>
    <module>ids</module>
    <module>gsuiteaddons</module>
    <module>eventarc</module>
    <module>essentialcontacts</module>
    <module>domains</module>
    <module>documentai</module>
    <module>dlp</module>
    <module>clouddebugger</module>
    <module>datastream</module>
    <module>metastore</module>
    <module>dataproc</module>
    <module>dataplex</module>
    <module>datafusion</module>
    <module>datacatalog</module>
    <module>containeranalysis</module>
    <module>container</module>
    <module>contactcenterinsights</module>
    <module>cloudbuild</module>
    <module>binaryauthorization</module>
    <module>automl</module>
    <module>assuredworkloads</module>
    <module>artifactregistry</module>
    <module>appengine</module>
    <module>apigateway</module>
    <module>accesscontextmanager</module>
    <module>accessapproval</module>
    <module>global-autoconfig-properties</module>
  </modules>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <!--
          Versions chosen to be the latest still compatible with Java 8.
          Scoping formatter to generated modules for now as the main repo was re-formatted
          through both automated (#868) and manual changes (#869), and running this formatter
          from the parent POM updates a large number of files.
          -->
          <groupId>com.coveo</groupId>
          <artifactId>fmt-maven-plugin</artifactId>
          <version>2.9.1</version>
          <configuration>
            <style>google</style>
            <verbose>true</verbose>
          </configuration>
          <dependencies>
            <dependency>
              <groupId>com.google.googlejavaformat</groupId>
              <artifactId>google-java-format</artifactId>
              <version>1.7</version>
            </dependency>
          </dependencies>
        </plugin>
      </plugins>
    </pluginManagement>
  </build>

</project>
