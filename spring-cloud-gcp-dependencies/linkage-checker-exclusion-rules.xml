<LinkageCheckerFilter>
	<LinkageError>
		<Source>
			<Package name="com.google.cloud.spring.autoconfigure"/>
		</Source>
		<Reason>
			Spring's autoconfigure classes work only when certain classes are available.
		</Reason>
	</LinkageError>
	<LinkageError>
		<Target>
			<Class name="org.springframework.integration.file.remote.handler.FileTransferringMessageHandler"/>
		</Target>
		<Source>
			<Class name="com.google.cloud.spring.storage.integration.outbound.GcsMessageHandler"/>
		</Source>
		<Reason>
			The source class is used only when the target class is available.
		</Reason>
	</LinkageError>
	<LinkageError>
		<Target>
			<Package name="org.springframework.integration"/>
		</Target>
		<Source>
			<Package name="com.google.cloud.spring.storage.integration"/>
		</Source>
		<Reason>
			Spring Integration is optional for Storage.
		</Reason>
	</LinkageError>
	<LinkageError>
		<Target>
			<Package name="org.springframework.web"/>
		</Target>
		<Source>
			<Package name="com.google.cloud.spring.logging"/>
		</Source>
		<Reason>
			Spring Web MVC is optional for Logging.
		</Reason>
	</LinkageError>
	<LinkageError>
		<Target>
			<Package name="net.logstash"/>
		</Target>
		<Source>
			<Package name="com.google.cloud.spring.logging"/>
		</Source>
		<Reason>
			Logstash is optional for Logging.
		</Reason>
	</LinkageError>
</LinkageCheckerFilter>